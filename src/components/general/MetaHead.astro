---
import { IMetaHead } from "@types";
import { info } from "@data";

const { title, description, ogImageUrl } = Astro.props as IMetaHead;
---

<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<!-- Favicon -->
<link rel="icon" type="image/x-icon" href="/assets/images/profile.png" />
<link rel="icon" type="image/png" sizes="32x32" href="/assets/images/profile.png" />
<link rel="icon" type="image/png" sizes="16x16" href="/assets/images/profile.png" />
<link rel="manifest" href="/site.webmanifest" />
<meta name="msapplication-TileColor" content="#da532c" />
<meta name="theme-color" content="#ffffff" />

<meta name="generator" content={Astro.generator} />

<!-- Primary Meta Tags -->